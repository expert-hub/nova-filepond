{
  "name": "expert-hub/nova-filepond",
  "description": "A Nova field for uploading File, Image and Video using filepond, for nova 5, cloned from digital-creative/nova-filepond.",
  "keywords": [
    "file",
    "upload",
    "video",
    "laravel",
    "nova",
    "nova4",
    "field"
  ],
  "authors": [
    {
      "name": "Rafael Milewski"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "laravel/nova": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "DigitalCreative\\Filepond\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DigitalCreative\\Filepond\\FilepondServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
